{
  "name": "my-vercel-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0",
    "build": "prisma generate --no-engine && next build",
    "vercel-build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "./scripts/studio.sh",
    "studio:direct": "DATABASE_URL=\"$DIRECT_URL\" prisma studio",
    "import:excel": "ts-node scripts/import-excel.ts",
    "migrate:export": "ts-node scripts/migrate-to-production.ts --export",
    "migrate:prod": "ts-node scripts/migrate-to-production.ts --migrate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:production": "./scripts/migrate-production.sh",
    "db:migrate": "prisma migrate dev",
    "db:seed:sample": "ts-node scripts/insert-sample-data.ts",
    "import:sample-1000": "ts-node scripts/import-sample-1000.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^5.22.0",
    "next": "15.4.5",
    "node-fetch": "^2.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
